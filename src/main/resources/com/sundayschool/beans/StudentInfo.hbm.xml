<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sundayschool.persistence.StudentInfo" table="ss_written_competition">
        <meta attribute="class-description">
            This class contains the employee detail.
        </meta>
        <id name="id" type="java.lang.Integer" column="id">
            <generator class="increment"/>
        </id>
        <property name="firstName" column="first_name" type="string"/>
        <property name="middleName" column="middle_name" type="string"/>
        <property name="lastName" column="last_name" type="string"/>
        <property name="region" column="region" type="string"/>
        <property name="church" column="church" type="string"/>
        <property name="categoryCode" column="category_code" type="string"/>
        <property name="ssGroup" column="ss_group" type="string"/>
        <property name="venue" column="venue" type="string"/>
        <property name="judge1Marks" column="judge1_marks" type="integer"/>
        <property name="judge2Marks" column="judge2_marks" type="integer"/>
        <property name="judge3Marks" column="judge3_marks" type="integer"/>
        <!--<property name="judge4Marks" column="judge4_marks" type="integer"/>-->
        <!--<property name="judge5Marks" column="judge5_marks" type="integer"/>-->
        <!--<property name="judge6Marks" column="judge6_marks" type="integer"/>-->
    </class>
</hibernate-mapping>